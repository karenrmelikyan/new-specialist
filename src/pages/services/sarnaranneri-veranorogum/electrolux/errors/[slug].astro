---
import WrapperContentIssuesSlugPage from '../../../../../components/WrapperContentIssuesSlugPage.astro'
import content from '../../../../../content/services/sarnaranneri-veranorogum/electrolux/errors/data.json'

export async function getStaticPaths() {
    return content.items.map(item => ({
        params: { slug: item.slug }
    }))
}

const { slug } = Astro.params
---

<WrapperContentIssuesSlugPage content={content} slug={slug} />