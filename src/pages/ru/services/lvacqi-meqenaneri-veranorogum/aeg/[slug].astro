import content from '../../../../../content/ru/services/lvacqi-meqenaneri-veranorogum/aeg/data.json'

export async function getStaticPaths() {
    return content.items.map(item => ({
        params: { slug: item.slug }
    }))
}

const { slug } = Astro.params
---

